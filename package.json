{
  "name": "projects-portfolio",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/datasource.ts && nest start --watch",
    "start:dev": "npm run build && typeorm -d ./dist/datasource.js migration:run && nest start --watch",
    "start:debug": "npm run migration:run && nest start --debug --watch",
    "start:prod": "npm run migration:run && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev": "docker-compose -f dev.yml up --build -V web-dev",
    "migration:run": "node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert ": "npm run build && node ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.3.3",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.3.3",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.3.3",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.3",
    "cookie-parser": "^1.4.6",
    "express-basic-auth": "^1.2.1",
    "handlebars": "^4.7.7",
    "joi": "^17.7.0",
    "logform": "^2.5.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "socket.io": "^4.6.0",
    "swagger-ui-express": "^4.6.0",
    "telegraf": "^4.11.2",
    "typeorm": "^0.3.11",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.4.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^18.11.19",
    "@types/supertest": "^2.0.12",
    "@types/triple-beam": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "nodemon": "^3.0.2",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
